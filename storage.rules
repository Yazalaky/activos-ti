rules_version = '2';
service firebase.storage {
  match /b/{bucket}/o {
    function isSignedIn() {
      return request.auth != null;
    }

    match /{allPaths=**} {
      // MVP: requiere autenticaci√≥n; ajusta por carpetas/roles cuando subas evidencias.
      allow read: if isSignedIn();
      allow write: if isSignedIn();
    }
  }
}

